.screen-income
  height: 100vh
  background: #F5F5F5;
  padding 10px 12px 0px 12px

  .fz11
    font-size: 11px;

  .fz13
    font-size: 13px;
    
  .fz26
    font-size: 26px;

  .color334
    color: #323334;

  .color636
    color: #363636

  .color030
    color: #303030

  .color300
    color: #FE1300

  .fw600
    font-weight: 600;

  .fw400
    font-weight: 400;

  .mr2
    margin-right 2px 

  .header
    background: #FFFFFF;
    border-radius: 7px;
    display flex
    justify-content space-between
    align-items center
    height: 87px
    padding 18px 14px
    box-sizing: border-box;

    .header-right
      background: linear-gradient(90deg, #FE7705 0%, #FE0700 100%);
      border-radius: 14px;
      width: 80px;
      height: 27px;
      line-height 27px
      text-align center
      color: #FFFFFF;

  .main
    margin-top 10px 
    background: #FFFFFF;
    border-radius: 7px;
    height: 201px;
    padding 18px 14px
    box-sizing: border-box;

    .main-header
      display: grid;
      grid-template-columns: 1fr 1fr;

    .main-content
      display flex
      justify-content space-between
      align-items flex-end
      margin-top 26px 

      .main-content-click
        width: 102px;
        height: 27px;
        background: linear-gradient(90deg, #FE7705 0%, #FE0700 100%);
        border-radius: 14px;
        line-height 27px
        text-align center
        color: #FFFFFF;

    .main-footer
      margin-top 20px 
      font-size: 11px;
      display flex
      justify-content center
      align-items center

      .main-footer-title
        
      span
        color: #1676FF;

  .footer
    margin-top 20px 

    .footer-title
      font-size: 11px;
      color: #1676FF;
      display flex
      justify-content center
      align-items center
      
    .footer-explain
      margin-top 30px 
      p
        font-size: 13px;
        color: #828282;
        line-height: 19px;
        margin-left 5px
        margin-right 5px
        margin-block-start 0px
        margin-block-end 0px
        font-weight: 400;

    
  
